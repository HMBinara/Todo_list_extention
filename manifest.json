{
    "manifest_version": 3,
    "name": "Binara's Task Overlay",
    "version": "1.2",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "style.css"
            ]
        }
    ]
}